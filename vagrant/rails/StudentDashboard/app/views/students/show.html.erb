<div class="container">
    <h2><%= @dojo.branch%>'s <%= @student.first_name %> <%= @student.last_name %></h2><br>
    <h4>Email: <%= @student.email %></h4><hr>
    <h3>Cohort</h3>
    <table class="table table-striped">
        <tbody>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <% @cohort.each do |student|%>
            <tr>
                <td><%= student.first_name %></td>
                <td><%= student.last_name %></td>
                <td><%= student.email %></td>
            </tr>
            <% end %>
        </tbody>
    </table>
    <p><a href='/dojos/<%=@dojo.id%>/students/<%=@student.id%>/edit'>Edit</a></p>
</div>